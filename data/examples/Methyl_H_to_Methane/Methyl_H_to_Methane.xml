        </property>
      </propertyList>
    </molecule>

  </moleculeList>

  <reactionList>

    <reaction id="R1" reversible="true">
      <reactant>
        <molecule ref="Methyl"  role="deficientReactant" />
      </reactant>

      <reactant>
        <molecule ref="H"  role="excessReactant" />
      </reactant>

      <product>
        <molecule ref="Methane"  role="modelled" />
      </product>

      <me:MCRCMethod xsi:type="me:MesmerILT">
        <me:preExponential units="cm3molecule-1s-1" lower="1.e-10" upper="5.e-10" stepsize="1.e-11">3.e-10</me:preExponential>
        <me:activationEnergy  units="cm-1">0.0</me:activationEnergy>
        <me:TInfinity>300.0</me:TInfinity>
        <me:nInfinity lower="0.4" upper="1.2" stepsize="0.01">0.8</me:nInfinity>
      </me:MCRCMethod>

    </reaction>

  </reactionList>

