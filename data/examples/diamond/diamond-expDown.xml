<?xml version="1.0" encoding="utf-8" ?>
<?xml-stylesheet type='text/xsl' href='../../mesmer2.xsl' media='other'?>
<?xml-stylesheet type='text/xsl' href='../../mesmer1.xsl' media='screen'?>
<me:mesmer xmlns="http://www.xml-cml.org/schema"
           xmlns:me="http://www.chem.leeds.ac.uk/mesmer"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <me:title>OH Acetylene association</me:title>
  <moleculeList>
    <molecule id="CH3">
      <propertyList>
        <property dictRef="me:ZPE">
          <scalar units="kJ/mol">0.0</scalar>
        </property>
        <property dictRef="me:MW">
          <scalar units="amu">15.0</scalar>
        </property>
      </propertyList>
      <me:DOSCMethod>ClassicalRotors</me:DOSCMethod>
    </molecule>
    <molecule id="activeSpaceRadical">
      <propertyList>
        <property dictRef="me:ZPE">
          <scalar units="kJ/mol">297.0</scalar>
        </property>
        <property dictRef="me:MW">
          <scalar units="amu">156.0</scalar>
        </property>
      </propertyList>
      <me:DOSCMethod>ClassicalRotors</me:DOSCMethod>
    </molecule>
    <molecule id="5AtomActiveSpace">
      <propertyList>
        <property dictRef="me:ZPE">
          <scalar units="kJ/mol">0.0</scalar>
        </property>
        <property dictRef="me:symmetryNumber">
          <scalar>1</scalar>
        </property>
        <property dictRef="me:vibFreqs">
          <array units="cm-1">
            107.255912
            342.337739
            361.562987
            404.568852
